<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FlexGrid101.CheckList"
             xmlns:ios="clr-namespace:Xamarin.Forms.PlatformConfiguration.iOSSpecific;assembly=Xamarin.Forms.Core" 
             ios:Page.UseSafeArea="true"
             xmlns:c1="http://schemas.grapecity.com/xf/2019/xaml">
    <c1:FlexGrid x:Name="grid" AutoGeneratingColumn="OnAutoGeneratingColumn">
        <c1:FlexGrid.Behaviors>
            <c1:CheckListBehavior SelectionBinding="Selected"/>
        </c1:FlexGrid.Behaviors>
    </c1:FlexGrid>
</ContentPage>